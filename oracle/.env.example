# Environment (local or test, defaults to local)
NODE_ENV=local

# Sepolia RPC URL (Required only when NODE_ENV=test)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR-API-KEY

# Neon Devnet RPC URL (Required only when NODE_ENV=test)
NEONDEVNET_RPC_URL=https://neon-devnet.blockscout.com/api/eth-rpc

# Anvil port (Optional, defaults to 8545)
ANVIL_PORT=8545
